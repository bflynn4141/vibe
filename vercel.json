    { "source": "/decks/archaeology", "destination": "https://manus.im/app/TQy7xqFUFBMMRuy22UeVns", "permanent": false }
  ],
  "rewrites": [
    { "source": "/style-demo", "destination": "/api/style-demo" },
    { "source": "/artifacts", "destination": "/api/artifacts-browse" },
    { "source": "/a/:slug", "destination": "/api/artifacts/:slug" },
    { "source": "/projects", "destination": "/api/projects-browse" },
    { "source": "/u/:handle", "destination": "/api/user-profile" },
    { "source": "/api/presence", "destination": "/api/presence.js" },
