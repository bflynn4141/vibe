{
  "buildCommand": "",
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/api/openapi.json",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/llms.txt",
      "headers": [
        { "key": "Content-Type", "value": "text/plain; charset=utf-8" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/.well-known/airc",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ],
  "redirects": [
    { "source": "/decks/origin", "destination": "https://manus.im/app/78Rqign33nm49Z8V41pmdV", "permanent": false },
    { "source": "/decks/pitch", "destination": "https://manus.im/app/gRpfjt2aZvbmckbJS7wDBa", "permanent": false },
    { "source": "/decks/entity", "destination": "https://manus.im/app/bz29w23bkcQVq8PLKhxUjq", "permanent": false },
